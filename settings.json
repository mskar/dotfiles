{
  "editor.accessibilitySupport" : "off",
  "editor.fontFamily" : "Ewka Nerd Font",
  "editor.fontLigatures" : true,
  "editor.fontSize" : 16,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled" : false,
  "editor.suggestSelection": "first",
  "editor.tabCompletion" : "on",
  "extensions.ignoreRecommendations" : false,
  "git.autofetch" : true,
  "jupyter.alwaysTrustNotebooks" : true,
  "jupyter.askForKernelRestart": false,
  "jupyter.interactiveWindowMode" : "perFile",
  "jupyter.sendSelectionToInteractiveWindow" : true,
  "python.analysis.completeFunctionParens" : true,
  "python.analysis.typeCheckingMode" : "basic",
  "python.autoComplete.addBrackets" : true,
  "python.defaultInterpreterPath" : "/usr/local/Caskroom/mambaforge/base/bin/python",
  "python.languageServer" : "Pylance",
  "r.alwaysUseActiveTerminal" : true,
  "r.bracketedPaste" : true,
  "r.rterm.mac" : "$(brew --prefix)\/Caskroom\/mambaforge\/base\/envs\/${workspaceFolderBasename}\/bin\/radian",
  "r.sessionWatcher" : true,
  "tabnine.experimentalAutoImports" : true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily" : "Ewka Nerd Font",
  "terminal.integrated.fontLigatures" : true,
  "terminal.integrated.fontSize" : 16,
  "terminal.integrated.macOptionIsMeta" : true,
  "vscode-neovim.neovimExecutablePaths.darwin" : "\/usr\/local\/bin\/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin" : "$HOME\/.config\/nvim\/init.vim",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "vim.easymotion": false,
  "vim.enableNeovim": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "window.zoomLevel": 0,
  "vim.normalModeKeyBindings": [
      {
          "before": [
              "<leader>",
              "t"
          ],
          "commands": [
              "workbench.action.terminal.focus"
          ]
      },
      {
          "before": ["Y"],
          "after": ["y$"]
      }
  ],
  "vim.insertModeKeyBindings": [
      {
          "before": [
              "<C-a>"
          ],
          "commands": [
              "cursorHome"
          ]
      },
      {
          "before": [
              "<C-b>"
          ],
          "commands": [
              "cursorLeft"
          ]
      },
      {
          "before": [
              "<C-d>"
          ],
          "commands": [
              "deleteRight"
          ]
      },
      {
          "before": [
              "<C-e>"
          ],
          "commands": [
              "cursorEnd"
          ]
      },
      {
          "before": [
              "<C-f>"
          ],
          "commands": [
              "cursorRight"
          ]
      },
      {
          "before": [
              "<C-h>"
          ],
          "commands": [
              "deleteLeft"
          ]
      },
      {
          "before": [
              "<C-k>"
          ],
          "commands": [
              "deleteAllRight"
          ]
      },
      {
          "before": [
              "<C-t>"
          ],
          "commands": [
              "editor.action.transposeLetters"
          ]
      },
      {
          "before": [
              "<C-u>"
          ],
          "commands": [
              "deleteAllLeft"
          ]
      },
      {
          "before": [
              "<C-w>"
          ],
          "commands": [
              "deleteWordLeft"
          ]
      },
      {
          "before": [
              "<A-b>"
          ],
          "commands": [
              "cursorWordLeft"
          ]
      },
      {
          "before": [
              "<A-c>"
          ],
          "commands": [
              "editor.action.transformToTitlecase"
          ]
      },
      {
          "before": [
              "<A-f>"
          ],
          "commands": [
              "cursorWordEndRight"
          ]
      },
      {
          "before": [
              "<A-h>"
          ],
          "commands": [
              "deleteWordLeft"
          ]
      },
      {
          "before": [
              "<A-l>"
          ],
          "commands": [
              "editor.action.transformToLowercase"
          ]
      },
      {
          "before": [
              "<A-u>"
          ],
          "commands": [
              "editor.action.transformToUppercase"
          ]
      },
      {
          "before": [
              "<A-_>"
          ],
          "commands": [
              "undo"
          ]
      }
  ],
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "git.confirmSync": false,
  "window.zoomLevel": 1
}
