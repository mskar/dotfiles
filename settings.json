{
  "editor.accessibilitySupport" : "off",
  "editor.fontFamily" : "FiraCode Nerd Font",
  "editor.fontLigatures" : true,
  "editor.fontSize" : 16,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled" : false,
  "editor.suggestSelection": "first",
  "editor.tabCompletion" : "on",
  "extensions.ignoreRecommendations" : false,
  "git.autofetch" : true,
  "jupyter.alwaysTrustNotebooks" : true,
  "jupyter.askForKernelRestart": false,
  "jupyter.interactiveWindowMode" : "perFile",
  "jupyter.sendSelectionToInteractiveWindow" : true,
  "python.analysis.completeFunctionParens" : true,
  "python.analysis.typeCheckingMode" : "basic",
  "python.autoComplete.addBrackets" : true,
  "python.defaultInterpreterPath" : "$(brew --prefix)\/Caskroom\/mambaforge\/base\/envs\/${workspaceFolderBasename}\/bin\/python",
  "python.languageServer" : "Pylance",
  "r.alwaysUseActiveTerminal" : true,
  "r.bracketedPaste" : true,
  "r.rterm.mac" : "$(brew --prefix)\/Caskroom\/mambaforge\/base\/envs\/${workspaceFolderBasename}\/bin\/radian",
  "r.sessionWatcher" : true,
  "tabnine.experimentalAutoImports" : true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily" : "FiraCode Nerd Font",
  "terminal.integrated.fontLigatures" : true,
  "terminal.integrated.fontSize" : 16,
  "terminal.integrated.macOptionIsMeta" : true,
  "vscode-neovim.neovimExecutablePaths.darwin" : "\/usr\/local\/bin\/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin" : "$HOME\/.config\/nvim\/init.vim",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.zoomLevel" : 3,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "git.confirmSync": false
}
